{
  "name": "@pureskillgg/awsjs",
  "version": "0.4.0",
  "description": "Node.js clients and utilities for AWS.",
  "type": "module",
  "main": "index.js",
  "exports": {
    "import": "./index.js"
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/pureskillgg/awsjs",
  "bugs": "https://github.com/pureskillgg/awsjs/issues",
  "repository": "pureskillgg/awsjs",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "PureSkill.gg",
    "email": "secure@pureskill.gg"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "data",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "pretest": "node index.js",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "npm-run-all --parallel lint:*",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "lint:js": "eslint --ignore-path .gitignore --ext .js .",
    "preversion": "npm-run-all --parallel lint test",
    "postversion": "git push --follow-tags",
    "example": "node examples",
    "example:inspect": "node --inspect examples",
    "format": "npm-run-all --parallel format:*",
    "preformat": "prettier --write --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "format:js": "eslint --ignore-path .gitignore --fix --ext .js .",
    "report": "nyc report"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "resolutions": {
    "aws-sdk": "2.804.0"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.11.0",
    "@aws-sdk/client-lambda": "^3.11.0",
    "@aws-sdk/client-sqs": "^3.11.0",
    "@meltwater/mlabs-logger": "^8.0.0",
    "@meltwater/phi": "^4.0.0",
    "@meltwater/tau": "^2.0.0",
    "aws-sdk": "^2.804.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@meltwater/examplr": "^6.0.0",
    "ava": "^3.3.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "testdouble": "^3.16.1"
  }
}
