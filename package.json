{
  "name": "@pureskillgg/awsjs",
  "version": "1.1.0",
  "description": "Clients and tools for building on AWS.",
  "type": "module",
  "main": "index.js",
  "exports": {
    "import": "./index.js"
  },
  "module": "index.js",
  "sideEffects": false,
  "keywords": [
    "node"
  ],
  "homepage": "https://github.com/pureskillgg/awsjs",
  "bugs": "https://github.com/pureskillgg/awsjs/issues",
  "repository": "pureskillgg/awsjs",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "PureSkill.gg",
    "email": "secure@pureskill.gg"
  },
  "files": [
    "yarn.lock",
    "index.js",
    "lib",
    "data",
    "docs"
  ],
  "scripts": {
    "test": "nyc ava",
    "pretest": "node index.js",
    "test:update": "ava --update-snapshots",
    "test:watch": "ava --watch",
    "test:debug": "ava debug --break",
    "lint": "npm-run-all --parallel lint:*",
    "lint:prettier": "prettier --check --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "lint:js": "eslint --ignore-path .gitignore --ext .js .",
    "lint:docs": "documentation lint 'lib/**/*.doc.js'",
    "preversion": "npm-run-all --parallel lint test",
    "postversion": "git push --follow-tags",
    "example": "node examples",
    "example:inspect": "node --inspect examples",
    "format": "npm-run-all --parallel format:*",
    "preformat": "prettier --write --ignore-path .gitignore '**/*.(js|json|yml|yaml|graphql)'",
    "format:js": "eslint --ignore-path .gitignore --fix --ext .js .",
    "docs": "documentation serve --watch --github 'lib/**/*.doc.js'",
    "docs:build": "documentation build --format html --github --output .docs 'lib/**/*.doc.js'",
    "report": "nyc report"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.20.0",
    "@aws-sdk/client-eventbridge": "^3.20.0",
    "@aws-sdk/client-lambda": "^3.20.0",
    "@aws-sdk/client-s3": "^3.20.0",
    "@aws-sdk/client-sqs": "^3.20.0",
    "@aws-sdk/lib-dynamodb": "^3.20.0",
    "@aws-sdk/util-dynamodb": "^3.20.0",
    "@meltwater/mlabs-logger": "^8.0.0",
    "@meltwater/phi": "^4.0.0",
    "@meltwater/tau": "^2.0.0",
    "change-case": "^4.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@meltwater/examplr": "^6.0.0",
    "ava": "^3.3.0",
    "documentation": "^13.2.5",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^15.0.0",
    "prettier": "^2.3.0",
    "testdouble": "^3.16.1"
  }
}
